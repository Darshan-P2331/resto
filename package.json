  },
  "homepage": "https://github.com/Darshan-P2331/resto#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.27.0",
    "concurrently": "^6.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "googleapis": "^89.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.11",
    "node-fetch": "^3.0.0",
    "nodemailer": "^6.7.0",
